        Executable: cafae64.exe
        CommandLine: -hive %sourceFile% -all_system -csv -dateformat dd-mm-yyyy -pair_datetime -no_whitespace
        ExportFormat: csv
        ExportFile: SYSTEM_Registry_Parsed.csv

