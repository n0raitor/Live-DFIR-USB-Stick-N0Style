  <!--FileItem-->
  <BaseAuditConfiguration Name="FileItem">
    <BaseAuditConfigurationModules>
      <BaseAuditConfigurationModule Name="files-api" Enabled="1" Title="File Listing (API Mode)">
        <BaseModuleParams>
          <BaseModuleParam ParamName="Path" ParamValue="%systemdrive%"/>
          <BaseModuleParam ParamName="Regex" ParamValue=""/>
					<BaseModuleParam ParamName="Depth" ParamValue="6"/>
          <BaseModuleParam ParamName="Strings" ParamValue="false"/>
          <BaseModuleParam ParamName="Include Files" ParamValue="true"/>
          <BaseModuleParam ParamName="Include Directories" ParamValue="true"/>
