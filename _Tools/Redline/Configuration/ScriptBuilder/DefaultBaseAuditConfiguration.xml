          <BaseModuleParam ParamName="Strings" ParamValue="false"/>
          <BaseModuleParam ParamName="Include Files" ParamValue="true"/>
          <BaseModuleParam ParamName="Include Directories" ParamValue="true"/>
        </BaseModuleParams>
      </BaseAuditConfigurationModule>
      <BaseAuditConfigurationModule Name="files-api" Title="File Listing (OS X)" TargetPlatform="OSX">
        <BaseModuleParams>
          <BaseModuleParam ParamName="Path" ParamValue="/" />
          <BaseModuleParam ParamName="Regex" ParamValue="^(?:Applications|Library|System|User|bin|cores|opt|private|sbin|usr)+" />
          <BaseModuleParam ParamName="Depth" ParamValue="" />
          <BaseModuleParam ParamName="Strings" ParamValue="false" />
          <BaseModuleParam ParamName="Include Files" ParamValue="true" />
          <BaseModuleParam ParamName="Include Directories" ParamValue="true" />
        </BaseModuleParams>
      </BaseAuditConfigurationModule>
      <BaseAuditConfigurationModule Name="files-raw" Enabled="0" Title="File Listing (OS X)" TargetPlatform="OSX">
        <BaseModuleParams>
          <BaseModuleParam ParamName="Path" ParamValue="/" />
          <BaseModuleParam ParamName="Regex" ParamValue="^(?:Applications|Library|System|User|bin|cores|opt|private|sbin|usr)+" />
          <BaseModuleParam ParamName="Depth" ParamValue="" />
          <BaseModuleParam ParamName="Strings" ParamValue="false" />
          <BaseModuleParam ParamName="Include Files" ParamValue="true" />
          <BaseModuleParam ParamName="Include Directories" ParamValue="true" />
